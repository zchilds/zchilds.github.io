        "front": "Sex",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>Sex</strong></p><p><span style=\"color:#000000\">&bull;Gender including transgender</span></p><p><span style=\"color:#000000\">&bull;Unwelcome sexual advances</span></p><p><span style=\"color:#000000\">&bull;Requests for sexual favors</span></p><p><span style=\"color:#000000\">&bull;Other verbal or physical harassment of a sexual nature</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Sexual Orientation",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>Sexual Orientation</strong></p><p><span style=\"color:#000000\">&bull;Includes lesbian, gay, bisexual, and transgender employees</span></p><p><span style=\"color:#000000\">&bull;Regardless of any contrary state or local laws</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Genetic Information",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>Genetic Information</strong></p><p><span style=\"color:#000000\">&bull;Information about an individual&rsquo;s genetic tests</span></p><p><span style=\"color:#000000\">&bull;The genetic tests of a family member</span></p><p><span style=\"color:#000000\">&bull;The manifestation of a disease or disorder in a family member</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      }
    ]
  },
