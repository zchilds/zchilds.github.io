  {
    "_id": "59b051dc877987bc144d2b05",
    "_layout": "full",
    "_type": "component",
    "_parentId": "59b0517b877987bc144d2b03",
    "body": "<p>The purpose of this training is to create a safe and healthy working environment for everyone.</p>",
    "displayTitle": "",
    "title": "ExposeBoxes 1",
    "_isAvailable": true,
    "_isOptional": false,
    "_classes": "",
    "_component": "expose",
    "_pageLevelProgress": {
      "_isEnabled": true
    },
    "instruction": "Click the boxes to reveal more information about the negative effects of workplace harassment.",
    "_columns": 4,
    "_animationType": "flipUp",
    "_items": [
      {
        "front": "Mental",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>Mental</strong></p><p><span style=\"color:#000000\">Leave lasting emotional damage</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Physical",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>Physical</strong></p><p><span style=\"color:#000000\">Stress and physical abuse may result</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Economic",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>Economic</strong></p><p><span style=\"color:#000000\">It can cost both the harasser and the employer large amounts of money</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Reputation",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>Reputation</strong></p><p><span style=\"color:#000000\">Reputations can be ruined, losing work and destroying careers</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      }
    ]
  },
