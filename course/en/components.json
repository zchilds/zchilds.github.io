  {
    "_id": "59b136dbfc94f1fc127eec53",
    "_layout": "full",
    "_type": "component",
    "_parentId": "59b136b2fc94f1fc127eec52",
    "body": "<p>Unwelcome conduct that is based on one or more of the following:</p>",
    "displayTitle": "Once you have finished with this section, scroll down for more.",
    "title": "Once you have finished with this section, scroll down for more.",
    "_isAvailable": true,
    "_isOptional": false,
    "_classes": "",
    "_component": "expose",
    "_pageLevelProgress": {
      "_isEnabled": true
    },
    "instruction": "Click on a topic for more detail.",
    "_columns": 3,
    "_animationType": "fade",
    "_items": [
      {
        "front": "Race",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>Race</strong></p><p><span style=\"color:#000000\">&bull;Person is of a certain race</span></p><p><span style=\"color:#000000\">&bull;Personal characteristics associated with race (such as hair texture, skin color, or certain facial features)</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Color",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>Color</strong></p><p><span style=\"color:#000000\">&bull;Skin color complexion</span></p><p><span style=\"color:#000000\">&bull;Married to (or associated with) a person of a certain race or color</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "National Origin",
        "back": {
          "title": "",
          "body": "<p style=\"text-align: center;\"><strong>National Origin</strong></p><p><span style=\"color:#000000\">&bull;From a particular country or part of the world</span></p><p><span style=\"color:#000000\">&bull;Ethnicity or accent</span></p><p><span style=\"color:#000000\">&bull;Appear to be of a certain ethnic background (even if they are not)</span></p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      }
    ]
  },
  {
    "_id": "59b137bbfc94f1fc127eec55",
    "_layout": "full",
    "_type": "component",
    "_parentId": "59b13781fc94f1fc127eec54",
