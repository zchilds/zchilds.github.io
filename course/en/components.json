    "_hasNavigationInTextArea": false,
    "_setCompletionOn": "allItems",
    "_items": [
      {
        "title": "",
        "body": "<p>Lack of Diversity</p>",
        "_graphic": {
          "src": "course/en/assets/d253aab7e03b1323c272d4804b16043847468116.jpg",
          "alt": "",
          "attribution": ""
